<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:bar id="winQuickSearch"
      skinset = "quicksearch"
      visible = "false"
      width   = "350"
      kbclose = "true"
      top     = "-27"
      right   = "30"
      zindex  = "10000"
      focussable = "true">
        <a:hbox edge="0 5 5 5">
            <a:textbox id="txtQuickSearch" flex="1" focusselect="true" skin="searchbox_textbox" skinset="default" class="quickSearch"  />
            <a:button onclick="require('ext/quicksearch/quicksearch').findprevious(1)" skin="btnquicksearchnav" class="btnquicksearchnavLeft withDivider"  />
            <a:button onclick="require('ext/quicksearch/quicksearch').findnext(1)" skin="btnquicksearchnav" class="btnquicksearchnavRight" />
            <a:button margin="0 0 0 5" skin="btnsearchicon" icon="rounded_close.png" onclick="
              require('ext/quicksearch/quicksearch').toggleDialog(-1);
            " />
            <div id="divSearchCount"><span class="searchIter" id="spanSearchIter">0</span><span class="searchTotal" id="spanSearchTotal">0</span></div>
        </a:hbox>
    </a:bar>
</a:application>
